{
  "name": "contest",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "run": "ts-node --esm src/index.run.ts",
    "test": "jest --silent"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.\\.?\\/.+)\\.jsx?$": "$1",
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.[tj]sx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    }
  },
  "engines": {
    "node": ">=10.12"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/js-beautify": "^1.13.3",
    "@types/node": "^20.1.5",
    "esbuild": "^0.12.5",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "breakdance": "^3.0.1",
    "js-beautify": "^1.14.8",
    "js-logger": "^1.6.1",
    "mdast-util-from-markdown": "^1.3.1"
  }
}
