{
  "name": "contest-parser",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest --silent",
    "build": "tsc"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "moduleNameMapper": {
      "^(\\.\\.?\\/.+)\\.jsx?$": "$1",
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.[tj]sx?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "modulePathIgnorePatterns": ["<rootDir>/dist/"]
  },
  "engines": {
    "node": ">=10.12"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^20.1.5",
    "esbuild": "^0.12.5",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "ah-shared": "audit-hero/ah-shared",
    "axios": "^1.4.0",
    "js-logger": "^1.6.1"
  }
}
